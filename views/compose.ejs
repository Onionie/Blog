<%- include("partials/header") -%>
<h1 id>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <h4>Title</h4>
    <input class="form-control" type="text" name="postTitle" autocomplete="off"/>
    <h4>Post</h4>
    <textarea class="form-control" type="text" name="postBody" rows="5" cols="30"> </textarea> <br><br>
    <button class="btn btn-large btn-info" type="submit" name="publish">Publish</button>
  </div>

</form>

<%- include("partials/footer") -%>
